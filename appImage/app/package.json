{
  "name": "app",
  "version": "1.0.0",
  "description": "Just an app",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "start:dev": "npm run build:dev && npm run serve:dev",
    "test": "node test.js",
    "build:server": "webpack --config ./webpack.node.config.js",
    "build:server:dev": "webpack --config ./webpack.node.config.js --watch",
    "build:client": "webpack -p",
    "build:client:dev": "webpack --watch",
    "build": "npm run build:server && npm run build:client",
    "build:dev": "npm run build:server:dev & npm run build:client:dev",
    "serve": "node dist/server.js",
    "serve:dev": "nodemon -L --watch dist dist/server.js"
  },
  "author": "thomasmars",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.18.0",
